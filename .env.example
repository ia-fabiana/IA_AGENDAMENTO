# Google Gemini API
API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google Calendar OAuth (Sprint 2)
GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5173/oauth2callback

# Encryption (Sprint 2)
# IMPORTANT: Use a strong, unique key in production (minimum 32 characters)
ENCRYPTION_KEY=your-secure-encryption-key-here-minimum-32-characters-long

# Logging (Sprint 2)
LOG_LEVEL=info
NODE_ENV=development

# Backend Server (Sprint 2)
SERVER_PORT=3001
FRONTEND_URL=http://localhost:5173

# Application
PORT=5173
